{
    "name": "substance.io",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "scripts": {
        "start": "npm run babel:watch & npm run css:watch",
        "hugo:serve": "hugo server --config site/config.toml",
        "babel:build": "babel js -d dist",
        "babel:watch": "babel -w js -d dist",
        "css:build": "sass scss:dist",
        "css:watch": "sass --watch scss:dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bisha0/substance.io.git"
    },
    "author": "https://github.com/bisha0",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bisha0/substance.io/issues"
    },
    "homepage": "https://github.com/bisha0/substance.io#readme",
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.13.1",
        "@babel/preset-env": "^7.13.5",
        "sass": "^1.32.8"
    },
    "dependencies": {}
}
